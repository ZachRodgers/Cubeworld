{
  "model": {
    "type": "minecraft:select",
    "model": "minecraft:item/name_tag",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
      {
        "when": {
          "text": "dinoland"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/dinoland"
        }
      },
      {
        "when": {
          "text": "piratecove"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/piratecove"
        }
      },
      {
        "when": {
          "text": "magmamines"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/magmamines"
        }
      },
      {
        "when": {
          "text": "crystalcaverns"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/crystalcaverns"
        }
      },
      {
        "when": {
          "text": "ghosttown"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/ghosttown"
        }
      },
      {
        "when": {
          "text": "casino"
        },
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/casino"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/name_tag"
    }
  }
}
